<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:srcCompat="@mipmap/ic_launcher"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="7dp"
        app:layout_constraintHorizontal_bias="0.029" />

    <TextView
        android:id="@+id/textRestaurantName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이름"
        android:layout_marginTop="18dp"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintLeft_toRightOf="@+id/imageView3"
        android:layout_marginLeft="8dp" />

    <TextView
        android:id="@+id/textRestaurantAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="주소"
        app:layout_constraintLeft_toRightOf="@+id/imageView3"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textRestaurantName" />

    <TextView
        android:id="@+id/textRestaurantPhone"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:text="전화번호"
        app:layout_constraintLeft_toRightOf="@+id/imageView3"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textRestaurantAddress" />

    <TextView
        android:id="@+id/textRestaurantOpen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintLeft_toRightOf="@+id/textRestaurantName"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textRestaurantAddress"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintHorizontal_bias="0.403" />

    <TextView
        android:id="@+id/textRestaurantclose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textRestaurantAddress"
        android:layout_marginLeft="75dp"
        app:layout_constraintBottom_toBottomOf="@+id/textRestaurantPhone"
        android:layout_marginBottom="7dp"
        app:layout_constraintVertical_bias="1.0" />

    <CheckBox
        android:id="@+id/RestuarantCheck"
        android:layout_width="53dp"
        android:layout_height="32dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="42dp"
        app:layout_constraintLeft_toRightOf="@+id/imageView3"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.959" />
</android.support.constraint.ConstraintLayout>

<!--text ="" 인 TextView들은 여는시간, 닫는시간 표시했던 TextView인데
도착시간 보내는걸 포기했기 때문에 쓸곳이 없어 text 속성을 빈칸으로 만들었음.
없애면 오류 잡아야되서 귀찮아서 안없앰
-이유정
-->